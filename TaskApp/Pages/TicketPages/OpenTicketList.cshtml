@page
@model TaskApp.Pages.TicketPages.OpenTicketListModel
@{
}


<form method="post">

    @for (int i = 0; i <Model.OpenTickets.Count; i++ )
    {
    <div>
        <tr>
            <td>
                <input asp-for="OpenTickets[i].Subject " type="text" placeholder="Subject" />
            </td>
            <td>
                <input asp-for="OpenTickets[i].Description" placeholder="Description" />
            </td>
            <td>
                <input asp-for="OpenTickets[i].OpenDate " placeholder="Date of Created" />

            </td>
            <td>
                <a asp-page="./TicketDetail" asp-route-id="@Model.OpenTickets[i].ID"> Set Tech Detail </a>
            </td>
        </tr>
    </div>
      }

</form>